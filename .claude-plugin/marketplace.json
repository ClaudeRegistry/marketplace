{
  "name": "clauderegistry",
  "owner": {
    "name": "Claude Registry",
    "email": "contact@clauderegistry.com",
    "url": "https://clauderegistry.com"
  },
  "metadata": {
    "description": "Discover plugins that supercharge your Claude Code workflow with advanced security, architecture, and performance insights",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "clauditor",
      "source": "./plugins/clauditor",
      "description": "Comprehensive code auditing and assessment plugin with security, architecture, and performance analysis capabilities",
      "version": "1.0.0",
      "author": {
        "name": "Claude Registry"
      },
      "license": "MIT",
      "keywords": [
        "security",
        "audit",
        "code-quality",
        "architecture",
        "performance",
        "assessment",
        "vulnerability",
        "compliance",
        "java",
        "remediation"
      ],
      "strict": false,
      "commands": [
        "./plugins/clauditor/commands/generic-assessment.md",
        "./plugins/clauditor/commands/architecture-assessment.md",
        "./plugins/clauditor/commands/java-security-assessment.md",
        "./plugins/clauditor/commands/generate-report.md"
      ]
    },
    {
      "name": "documate",
      "source": "./plugins/documate",
      "description": "Intelligent documentation automation plugin for generating, maintaining, and syncing code documentation across your entire codebase",
      "version": "1.0.0",
      "author": {
        "name": "Claude Registry"
      },
      "license": "MIT",
      "keywords": [
        "documentation",
        "docs",
        "readme",
        "api-docs",
        "openapi",
        "swagger",
        "code-comments",
        "jsdoc",
        "inline-docs",
        "architecture",
        "diagrams",
        "markdown",
        "automation"
      ],
      "strict": false,
      "commands": [
        "./plugins/documate/commands/api-docs.md",
        "./plugins/documate/commands/architecture-diagram.md",
        "./plugins/documate/commands/doc-generate.md",
        "./plugins/documate/commands/doc-sync-check.md",
        "./plugins/documate/commands/doc-update.md",
        "./plugins/documate/commands/explain-code.md",
        "./plugins/documate/commands/readme-generate.md"
      ]
    }
  ]
}
